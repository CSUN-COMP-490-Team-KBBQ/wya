{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "compilerOptions": {
    "allowUnreachableCode": false,
    "composite": false,
    "strict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "module": "esnext",
    "moduleResolution": "node",
    "declaration": true,
    "declarationMap": true,
    "newLine": "lf",
    "removeComments": true,
    "allowJs": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules"]
}
