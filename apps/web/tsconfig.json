{
  "include": ["src"],
  "exclude": ["node_modules", "dist", "build"],
  "extends": "tsconfig/react-base.json",
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES5",
    "allowJs": true,
    "jsx": "react",
    "module": "ES6",
    "moduleResolution": "Node",
    "noEmit": false,
    "downlevelIteration": true,
    "importHelpers": true
  }
}
